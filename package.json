{
    "name": "sauce-cypress",
    "version": "1.0.0",
    "scripts": {
        "cypress:open": "npx cypress open",
        "cypress:run": "npx cypress run --reporter mochawesome --reporter-options reportDir=cypress-reports,reportFilename=mochawesome-report,overwrite=false,html=false,json=true",
        "report:generate": "npx mochawesome-merge cypress-reports/*.json > cypress-reports/merged.json && npx marge cypress-reports/merged.json -f report -o cypress-reports/html"
    },
    "devDependencies": {
        "cypress": "^14.5.4",
        "mochawesome": "^7.1.3",
        "mochawesome-merge": "^4.4.1",
        "mochawesome-report-generator": "^6.2.0"
    },
    "description": "",
    "main": "cypress.config.js",
    "keywords": [],
    "author": "",
    "license": "ISC"
}
